{
  "name": "erfanmomeniii/magic-policy",
  "description": "magic policy for laravel project methods",
  "keywords": [
    "policy",
    "magic policy",
    "laravel policy"
  ],
  "homepage": "https://github.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "erfan momeni",
      "email": "erfammioi@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "illuminate/support": ">=4",
    "doctrine/dbal": "^v2.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  },
  "autoload": {
    "psr-4": {
      "erfanmomeniii\\magicpolicy\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "erfanmomeniii\\magicpolicy\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers" : [
        "erfanmomeniii\\magicpolicy\\ServiceProvider\\MagicPolicyServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}